[run]
branch = True
source = nextcloud_contacts_to_gequdio

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    setup.py
    */tests.py
    */tests/*
    */migrations/*
    doc/*
    */scripts/*
